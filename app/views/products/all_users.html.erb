<h1>Listing Users</h1> 
<table border ="1">
  <tr>
    <th> First Name </th>
    <th> Last Name </th>
    <th> Email </th>
    <th> Delete </th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
    <td> <%= link_to 'Delete User', destroy_user_path(user), data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>